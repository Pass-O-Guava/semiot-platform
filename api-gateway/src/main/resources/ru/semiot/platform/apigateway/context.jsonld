{
    "@context": {
        "vocab": "${VOCABULARY_URL}#",
        "hydra": "http://www.w3.org/ns/hydra/core#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
    },
    "@graph": [
        {
            "@id": "vocab:EntryPoint",
            "@type": "hydra:Class",
            "hydra:supportedOperation": [
                {
                    "@type": "hydra:Operation",
                    "hydra:method": "GET",
                    "hydra:returns": "vocab:EntryPoint"
                }
            ],
            "hydra:supportedProperty": [
                {
                   "hydra:property": {
                        "@id": "vocab:EntryPoint/systems",
                        "@type": "hydra:Link",
                        "domain": "vocab:EntryPoint",
                        "range": "vocab:SystemCollection",
                        "supportedOperation": [
                            {
                                "@type": "hydra:Operation",
                                "method": "GET",
                                "returns": "vocab:SystemCollection"
                            }
                        ]
                    },
                    "readonly": true,
                    "writeonly": false 
                }
            ]
        },
        {
            "@id": "vocab:SystemCollection",
            "@type": "hydra:Class",
            "rdfs:subClassOf": "hydra:Collection",
            "hydra:supportedOperation": [
                {
                    "@type": "hydra:Operation",
                    "hydra:method": "GET",
                    "hydra:returns": "vocab:SystemCollection"
                }
            ]
        },
        {
            "@id": "vocab:observations",
            "@type": "hydra:Link",
            "rdfs:range": "vocab:WebSocketResource",
            "hydra:supportedOperation": [
                "@type": "vocab:WebSocketOperation",
                "hydra:returns": "ssn:Observation"
            ]
        },
        {
            "@id": "vocab:WebSocketResource",
            "@type": "hydra:Class",
            "rdfs:subClassOf": "hydra:Resource"
        },
        {
            "@id": "vocab:WebSocketOperation",
            "@type": "hydra:Class",
            "rdfs:subClassOf": "hydra:Operation"
        }
    ]
}
