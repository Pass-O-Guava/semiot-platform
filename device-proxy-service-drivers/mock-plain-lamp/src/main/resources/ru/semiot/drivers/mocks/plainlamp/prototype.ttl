@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ssn: <http://purl.oclc.org/NET/ssnx/ssn#> .
@prefix dul: <http://www.loa-cnr.it/ontologies/DUL.owl#> .
@prefix qudt-quantity: <http://qudt.org/vocab/quantity#> .
@prefix qudt-quantity-ext: <http://w3id.org/qudt/vocab/quantity/ext#> .
@prefix qudt-unit: <http://qudt.org/vocab/unit#> .
@prefix semiot: <http://w3id.org/semiot/ontologies/semiot#> .
@prefix proto: <http://w3id.org/semiot/ontologies/proto#> .
@prefix : <https://raw.githubusercontent.com/semiotproject/semiot-platform/master/device-proxy-service-drivers/mock-plain-lamp/src/main/resources/ru/semiot/drivers/mocks/plainlamp/prototype.ttl#> .

:PlainLamp a ssn:System, proto:Prototype ;
  rdfs:label "A plain lamp"^^xsd:string ;
  ssn:hasSubSystem :PlainLamp-Actuator ;
  ssn:implements :PlainLamp-Shine, :PlainLamp-Idle, :PlainLamp-Command-Switch-Operation .

:PlainLamp-Command-Switch-Operation a semiot:SwitchOperationCommand ;
  semiot:isSupportedBy :PlainLamp-Shine, :PlainLamp-Idle .

:PlainLamp-Shine a semiot:ActuationOperation ;
  semiot:supports :PlainLamp-Command-Switch-Operation ;
  dul:involvesAgent :PlainLamp-Actuator ;
  dul:hasParameter :PlainLamp-Shine-Lumen ;
  dul:hasParameter :PlainLamp-Shine-Color .

:PlainLamp-Shine-Lumen a dul:Parameter, semiot:PropertyMappingParameter ;
  semiot:isConsumedBy :PlainLamp-Actuator ;
  ssn:forProperty qudt-quantity:LuminousFlux ;
  dul:hasParameterDataValue "890"^^xsd:integer ;
  dul:isClassifiedBy qudt-unit:Lumen .

:PlainLamp-Shine-Color a dul:Parameter, semiot:PropertyMappingParameter ;
  semiot:isConsumedBy :PlainLamp-Actuator ;
  ssn:forProperty qudt-quantity-ex:CorrelatedColorTemperature ;
  dul:hasParameterDataValue "4000"^^xsd:integer ;
  dul:isClassifiedBy qudt-unit:Kelvin .

:PlainLamp-Idle a semiot:IdleOperation ;
  semiot:supports :PlainLamp-Command-Switch-Operation .

:PlainLamp-Actuator a semiot:ActuatingDevice, proto:Prototype ;
  semiot:actsOn qudt-quantity:LuminousFlux ;
  semiot:actsOn qudt-quantity-ext:CorrelatedColorTemperature ;
  semiot:hasActuatingCapability [
    a semiot:ActuatingCapability ;
    ssn:forProperty qudt-quantity:LuminousFlux ;
    semiot:hasActuatingProperty [
      a semiot:ActuatingProperty, semiot:ActuatingValueRange ;
      dul:hasDataValue "0"^^xsd:integer ;
      dul:hasDataValue "890"^xsd:integer ;
      dul:isClassifiedBy qudt-unit:Lumen ;
    ] ;
  ] ;
  semiot:hasActuatingCapability [
    a semiot:ActuatingCapability ;
    semiot:forProperty qudt-quantity-ex:CorrelatedColorTemperature ;
    semiot:hasActuatingProperty [
      a semiot:ActuatingProperty, semiot:ActuatingValueRange ;
      dul:hasDataValue "2700"^^xsd:integer ;
      dul:hasDataValue "6500"^xsd:integer ;
      dul:isClassifiedBy qudt-unit:Kelvin ;
    ] ;
  ] ;
.