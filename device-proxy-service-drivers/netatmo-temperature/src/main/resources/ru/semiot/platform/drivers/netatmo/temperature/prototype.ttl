@prefix : <https://raw.githubusercontent.com/semiotproject/semiot-platform/master/device-proxy-service-drivers/netatmo-temperature/src/main/resources/ru/semiot/platform/drivers/netatmo/temperature/prototype.ttl#> .
@prefix ssn: <http://purl.oclc.org/NET/ssnx/ssn#> .
@prefix qudt: <http://qudt.org/schema/qudt#> .
@prefix qudt-quantity: <http://qudt.org/vocab/quantity#> .
@prefix qudt-unit: <http://qudt.org/vocab/unit#> .
@prefix proto: <http://w3id.org/semiot/ontologies/proto#> .

:WeatherStation a ssn:System, proto:Prototype ;
    ssn:hasSubSystem :WeatherStation-TemperatureSensor ;
    .

:WeatherStation-TemperatureSensor a ssn:SensingDevice, proto:Prototype ;
    ssn:observes qudt-quantity:ThermodynamicTemperature ;
    ssn:hasMeasurementCapability [
        a ssn:MeasurementCapability ;
        ssn:forProperty qudt-quantity:ThermodynamicTemperature ;
        ssn:hasMeasurementProperty [
            a qudt:Unit ;
            ssn:hasValue [
                a qudt:Quantity ;
                ssn:hasValue qudt-unit:DegreeCelsius ;
            ] ; 
        ] ;
    ] ;
    .